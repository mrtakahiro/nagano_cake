<h3><%= @customers.last_name %> <%= @customers.first_name %>さんの会員情報編集</h3>
<%= form_with model: @customers, url: admin_customer_path(@customers.id), method: :patch do |f| %>

<table>
  <tr>
    <td><span>会員ID</span><%= f.text_field :id %></td>
  </tr>

  <tr>
    <td><span>氏名</span><%= f.text_field :last_name %><%= f.text_field :first_name %></td>
  </tr>

  <tr>
    <td><span>フリガナ</span><%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %></td>
  </tr>

  <tr>
    <td><span>郵便番号</span><%= f.text_field :postal_code %></td>
  </tr>

  <tr>
    <td><span>住所</span><%= f.text_field :address,class: "selector" %></td>
  </tr>

  <tr>
    <td><span>電話番号</span><%= f.text_field :telephone_number %></td>
  </td>

   <tr>
    <td><span>メールアドレス</span><%= f.text_field :email %></td>
  </td>

  <tr>
    <td><%= f.submit '変更を保存' %></td>
  </tr>
</table>
<% end %>