<h3>会員一覧</h3>

<table border="1" style="border-collapse: collapse">
  <thead>
    <tr>
      <th>会員ID</th> <th>氏名</th> <th>メールアドレス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.last_name %> <%= customer.first_name %></td>
      <td><%= customer.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>